---
import { getSession } from "auth-astro/server";

import Header from "@/components/Header.astro";
import HeroVotacion from "@/components/votacion/HeroVotacion.astro";
import HeroLoged from "@/components/votacion/HeroLoged.astro";
import MainLayout from "@/layouts/MainLayout.astro";
const session = await getSession(Astro.request);
---

<MainLayout title="CAMPTII V5.0">
  <Header />
  {session === null ? <HeroVotacion /> : <HeroLoged />}
</MainLayout>
